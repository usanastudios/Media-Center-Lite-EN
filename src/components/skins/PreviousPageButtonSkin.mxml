<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark" xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:d="http://ns.adobe.com/fxg/2008/dt" rollOverColor.over="#C71F1F">
	<s:transitions>
		<s:Transition fromState="up" toState="over" autoReverse="true">
			<s:Parallel/>
		</s:Transition>
	</s:transitions>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:BitmapImage 
		   source.up="@Embed('/assets/skins/previous_page_btn.png')" 
		   source.over="@Embed('/assets/skins/previous_page_over_btn.png')" 
		   source.down="@Embed('/assets/skins/previous_page_over_btn.png')"
		
		resizeMode="scale" />
</s:Skin>
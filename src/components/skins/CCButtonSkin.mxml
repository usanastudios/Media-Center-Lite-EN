<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark" xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:d="http://ns.adobe.com/fxg/2008/dt" >
	<s:transitions>
		<s:Transition fromState="up" toState="over" autoReverse="true">
			<s:Parallel/>
		</s:Transition>
	</s:transitions>
	<s:states>
		<s:State name="upAndSelected"/>
		<s:State name="down"/>
		<s:State name="downAndSelected"/>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="disabled"/>
		<s:State name="disabledAndSelected"/>
		<s:State name="overAndSelected"/>
	</s:states>
	<fx:Metadata>[HostComponent("spark.components.ToggleButton")]</fx:Metadata>
	<s:BitmapImage source.disabled ="@Embed('/assets/skins/ccBtn_disabled.png')" resizeMode="scale" />
	<s:BitmapImage source.disabledAndSelected ="@Embed('/assets/skins/ccBtn_disabled.png')" resizeMode="scale" />
	<s:BitmapImage source.down ="@Embed('/assets/skins/ccBtn_on.png')" resizeMode="scale" />
	<s:BitmapImage source.downAndSelected ="@Embed('/assets/skins/ccBtn_on.png')" resizeMode="scale" />
	<s:BitmapImage source.upAndSelected ="@Embed('/assets/skins/ccBtn_on.png')" resizeMode="scale" />
	<s:BitmapImage source.up="@Embed('/assets/skins/ccBtn.png')" resizeMode="scale" />
	<s:BitmapImage source.over="@Embed('/assets/skins/ccBtn_over.png')" resizeMode="scale" />
	<s:BitmapImage source.overAndSelected ="@Embed('/assets/skins/ccBtn_on.png')" resizeMode="scale" />
</s:Skin>


